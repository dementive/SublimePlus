---
name: Notes Syntax
scope: text.notes
hidden: true

contexts:
  main:
    - match: \.
      scope: list.element.dot

    - match: \b1\b\.
      scope: list.element.start

    - match: \b5\b\.
      scope: list.element.fifth

    - match: \b4\b\.
      scope: list.element.fourth

    - match: ---|\b3\b\.
      scope: list.element.third

    - match: --|\b2\b\.
      scope: list.element.second

    - match: ->|-|â€¢
      scope: list.element.dot
